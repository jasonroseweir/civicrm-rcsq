<?xml version="1.0"?>
<extension key="uk.co.jasonroseweir.civicrm.ext.rcsq" type="module">
  <file>rcsq</file>
  <name>CiviCRM RCSQ Extension</name>
  <description>A new custom CiviCRM extension, to provided additional functionality to help the management of the events where funds are being raised by multiple individuals for multiple Charities. Also to provide integration with JustGiving to create fund raising pages.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jason Roseweir</author>
    <email>jason@roseweir.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/jasonroseweir/uk.co.jasonroseweir.civicrm.ext.rcsq</url>
    <url desc="Documentation">https://github.com/jasonroseweir/uk.co.jasonroseweir.civicrm.ext.rcsq</url>
    <url desc="Support">https://github.com/jasonroseweir/uk.co.jasonroseweir.civicrm.ext.rcsq</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2016-01-24</releaseDate>
  <version>0.0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.2</ver>
  </compatibility>
  <comments>Module still a WIP</comments>
  <civix>
    <namespace>CRM/Rcsq</namespace>
  </civix>
</extension>
